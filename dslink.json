{
  "name": "dslink-dart-rpi",
  "version": "0.0.1",
  "description": "Raspberry Pi DSLink",
  "main": "bin/run.dart",
  "engines": {
    "dart": ">=1.12.0"
  },
  "getDependencies": [
    "bash tool/getdeps.sh"
  ],
  "configs": {
    "broker": {
      "type": "url"
    },
    "name": {
      "type": "string",
      "default": "RaspberryPi"
    },
    "token": {
      "type": "string"
    }
  },
  "sudo": true
}
